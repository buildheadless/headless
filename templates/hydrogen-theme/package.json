{
  "name": "hydrogen-theme",
  "private": true,
  "sideEffects": false,
  "version": "0.0.0",
  "scripts": {
    "build": "shopify hydrogen build",
    "dev": "shopify hydrogen dev --codegen",
    "dev:all": "run-p dev sanity:dev",
    "preview": "npm run build && shopify hydrogen preview",
    "lint": "eslint --no-error-on-unmatched-pattern --ext .js,.ts,.jsx,.tsx .",
    "format": "prettier --write --ignore-unknown .",
    "format:check": "prettier --check --ignore-unknown .",
    "typecheck": "tsc --noEmit",
    "sanity:dev": "cd ./studio && run-s dev",
    "sanity:deploy": "cd ./studio && run-s deploy"
  },
  "dependencies": {
    "@portabletext/react": "^3.0.11",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@remix-run/react": "2.3.1",
    "@sanity/client": "^6.9.1",
    "@sanity/image-url": "^1.0.2",
    "@sanity/overlays": "^2.0.4",
    "@sanity/react-loader": "^1.1.3",
    "@shopify/cli": "3.51.2",
    "@shopify/cli-hydrogen": "^6.0.2",
    "@shopify/hydrogen": "^2023.10.2",
    "@shopify/remix-oxygen": "^2.0.1",
    "class-variance-authority": "^0.7.0",
    "cva": "1.0.0-beta.1",
    "framer-motion": "^10.16.12",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "groq": "^3.20.2",
    "groqd": "^0.15.10",
    "isbot": "^3.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "2.3.1",
    "@remix-run/eslint-config": "^2.3.1",
    "@shopify/oxygen-workers-types": "^3.17.3",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/eslint": "^8.44.8",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "eslint": "^8.54.0",
    "eslint-plugin-hydrogen": "^0.12.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "0.5.7",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=16.13"
  },
  "browserslist": [
    "defaults"
  ]
}
