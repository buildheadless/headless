{
  "name": "headless.build",
  "version": "0.0.1",
  "license": "MIT",
  "packageManager": "pnpm@8.9.2",
  "scripts": {
    "build": "turbo build --filter=create-headless",
    "dev": "turbo dev",
    "dev:theme": "turbo dev --filter=hydrogen-theme --filter=sanity-studio",
    "deploy:studio": "turbo run deploy --filter=sanity-studio",
    "changeset": "changeset add",
    "version": "changeset version",
    "postinstall": "manypkg check",
    "fix": "manypkg fix"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.2",
    "@manypkg/cli": "^0.21.0",
    "tsup": "^7.2.0",
    "turbo": "^1.10.16"
  },
  "pnpm": {
    "patchedDependencies": {
      "@sanity/react-loader@1.1.0": "patches/@sanity__react-loader@1.1.0.patch"
    }
  }
}
